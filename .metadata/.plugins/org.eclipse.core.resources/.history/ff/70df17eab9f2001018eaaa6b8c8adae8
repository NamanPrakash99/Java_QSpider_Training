package Collections;
import java.util.*;

public class Example {

	public static void main(String[] args) {
		// creating non generic type of ArrayList
		ArrayList al=new ArrayList();

		al.add("Naman");
		al.add(2345);
		al.add(true);
		al.add("hello");
		al.add(987654);
		
		System.out.println(al);
	}

}
