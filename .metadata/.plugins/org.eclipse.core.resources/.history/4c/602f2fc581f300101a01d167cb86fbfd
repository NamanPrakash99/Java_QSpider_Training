package Collections;
import java.util.LinkedList;

public class LinkedListExample {

	public static void main(String[] args) {
		  LinkedList<String> list = new LinkedList<>();

	        list.add("Java");
	        list.add("Python");
	        list.add("C++");

	        System.out.println(list); 

	        System.out.println(list.get(1)); 
	}

}
