package Collections;
import java.util.LinkedList;

public class LinkedListExample {

	public static void main(String[] args) {
		  LinkedList list = new LinkedList();

	        list.add("Hello");
	        list.add("Naman");
	        list.add("Ravi");

	        System.out.println(list); 
	        System.out.println(list.get(1)); 
	        
	        list.add(10);
	        list.addFirst(5);
	        list.addLast(20);
	}

}
