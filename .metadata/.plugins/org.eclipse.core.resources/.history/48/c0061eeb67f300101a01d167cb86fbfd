package Collections;
import java.util.ArrayList;

public class Example4 {

	public static void main(String[] args) {
		ArrayList<Integer> al=new ArrayList<>();
		al.add(87);
		al.add(75);
		al.add(53);
		al.addFirst(88);
		al.addLast(90);
		
		System.out.println(al);
		
		
		ArrayList<String> al1=new ArrayList<>();
		al1.add("Blue");
		al1.add("Red");
		al1.add("Pink");
		al1.add("Green");
		al1.add("Black");
		System.out.println(al1);

	}

}
