package Collections;

public class Chocolate implements Comparable<>{
	int cid;
	String name;
	int price;
	
	Chocolate(int cid, String name,int price){
		this.cid=cid;
		this.name=name;
		this.price=price;
	}
	
	int GetId() {
		return cid;
	}
	String GetName() {
		return name;
	}
	int GetPrice() {
		return price;
	}
	@Override
    public String toString() {
        return "ID: " + cid + " | Name: " + name + " | Price: " + price;

	}
}
