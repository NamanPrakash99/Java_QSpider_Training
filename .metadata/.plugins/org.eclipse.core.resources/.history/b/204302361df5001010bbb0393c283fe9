package Collections;
import java.util.ArrayList;

public class TrainerDriver {

	public static void main(String[] args) {
		ArrayList<Trainer> a=new ArrayList<>();
		a.add(new Trainer(404,"Abc","Java"));
		a.add(new Trainer(443,"Ravi","SQL"));
		a.add(new Trainer(497,"Naman","Selenium"));
		a.add(new Trainer(432,"Shraddha","J2EE"));
		
		for(Trainer t:a) {
			System.out.println(t);
		}
		
		Collections.sort(a);
	}

}
