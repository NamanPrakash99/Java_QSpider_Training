package NonPrimitiveUpcasting;

public class Driver {

	public static void main(String[] args) {		
		
		//	Person p=new Employee("Ravi","Azamgarh",101,"Devloper")

		Employee e=new Employee("Ravi","Azamgarh",101,"Web-Developer");
		Person p=e;      // Upcasted Scenario
		
		Employee e1=(Employee) p;  // downcasting the variable for whcih the upcasting is done
		
		
		
		
		System.out.println(e.name);
		System.out.println(e.address);
		System.out.println(e.empid); // child memeber cannot access after doing upcasting to use do downcasting
		System.out.println(e.role);
		
		
		
//		System.out.println(e1.name);
//		System.out.println(e1.address);
//		System.out.println(e1.empid); // child memeber cannot access after doing upcasting to use do downcasting
//		System.out.println(e1.role);
		
	}

}
