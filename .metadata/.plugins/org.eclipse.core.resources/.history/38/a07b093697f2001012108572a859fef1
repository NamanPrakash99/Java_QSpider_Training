package WrapperClass;

public class Example {

	public static void main(String[] args) {
		
		// Auto Boxing
		int b=56;
		Integer i=b;
		System.out.println(i);
		
		short a=98;
		Short c=a;
		System.out.println(c);
		
		float f=78.6f;
		Float g=f;
		System.out.println(g);
		
		char t='a';
		Character q=t;
		System.out.println(q);
		
		byte m=76;
		Byte n=m;
		System.out.println(n);
		
		// Auto UnBoxing
//		Integer a=78;
//		int s=a;
//		System.out.println(s);
	}

}
