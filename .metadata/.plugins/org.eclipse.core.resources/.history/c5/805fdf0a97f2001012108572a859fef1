package WrapperClass;

public class Example {

	public static void main(String[] args) {
		
		// Auto Boxing
		int b=56;
		Integer i=b;
		System.out.println(i);
		
		short a=98;
		Short c=a;
		System.out.println(c);
		
		// Auto UnBoxing
		Integer a=78;
		int s=a;
		System.out.println(s);
	}

}
