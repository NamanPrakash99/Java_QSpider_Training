package Collections;
import java.util.Vector;

public class vecExample {

	public static void main(String[] args) {
		Vector v1=new Vector();  // no size specified
		v1.add(4);
		v1.add("naman");
		v1.add(9876);
		v1.add("hello");
		System.out.println(v1);
		
		Vector v2=new Vector();
		v2.add(8765);
		v2.add("heyyy");
		v2.add(7654);
		v2.add("hello");
		System.out.println(v2);
		
		v1.add(1,"ravi");
		v2.add(2,"hieeee");
		System.out.println(v1);
		System.out.println(v2);
		
		v1.addAll(v2);
		System.out.println(v1);
		
		
		
	}

}
