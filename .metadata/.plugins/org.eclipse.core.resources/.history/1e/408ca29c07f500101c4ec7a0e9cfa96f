package Collections;
import java.util.ArrayList;
import java.util.Iterator;

public class ChocolateDriver {

	public static void main(String[] args) {
		ArrayList<Chocolate> c=new ArrayList<>();
		c.add(new Chocolate(101,"Dairy Milk",200));
		c.add(new Chocolate(102,"5-Star",50));
		c.add(new Chocolate(103,"Ferrero Rocher",1000));
		c.add(new Chocolate(104,"Munch",20));
		c.add(new Chocolate(105,"Kitkat",80));
		
		Iterator<Chocolate> it=c.iterator();
		while(it.hasNext()) {
			Chocolate cr=(chocolate) it.next();
			System.out.println();
		}
		
	}

}
