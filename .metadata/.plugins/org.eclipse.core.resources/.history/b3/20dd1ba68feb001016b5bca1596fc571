package NonPrimitiveUpcasting;

public class Driver {

	public static void main(String[] args) {		
		
		//	Person p=new Employee("Ravi","Azamgarh",101,"Devloper")

		Employee e=new Employee("Ravi","Azamgarh",101,"Web-Developer");
		Person p=e;      // Upcasted Scenario
		
		// Downcasting the upcasted variable
		Employee e1=(Employee) p;  // downcasting the variable for which the upcasting is done
		
		
		
		
		
		System.out.println(e1.name);
		System.out.println(e1.address);
		System.out.println(e1.empid); // child member cannot access after doing upcasting to use do downcasting
		System.out.println(e1.role);
		
	}

}
