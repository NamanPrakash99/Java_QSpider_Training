package Collections;

public class EmployeeCompare implements Comparable<EmployeeCompare>{
	int eid;
	String ename;
	
	EmployeeCompare(int eid,String ename){
		this.eid=eid;
		this.ename=ename;
	}
	public String toString() {
        return "ID: " + eid + " | Name: " + ename;

	}
	
	// override the compareTo
	@Override
	public int compareTo(EmployeeCompare e) {
		return this.ename.compareTo(e.ename);   // ascending Order
		retrun e.ename.compareTo(this.name);
	}
}
