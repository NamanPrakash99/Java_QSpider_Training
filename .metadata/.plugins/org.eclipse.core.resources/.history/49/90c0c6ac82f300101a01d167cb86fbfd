package Collections;
import java.util.LinkedList;

public class LinkedListExample {

	public static void main(String[] args) {
		  LinkedList list = new LinkedList();

		  // add
	        list.add("Hello");
	        list.add("Naman");
	        list.add("Ravi");
	        
	        list.add(10);
	        list.addFirst(5);
	        list.addLast(20);
	        System.out.println(list);
	        
	        System.out.println(list.get(1)); 
	        
	        // remove
	        
	        list.remove("Naman");
	        list.remove(1);
	        list.removeFirst();
	        list.removeLast();
	        System.out.println(list);
	        
	        //update
	        list.set(1, "Naman");
	        System.out.println();list
	}

}
