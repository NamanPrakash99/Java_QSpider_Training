package Collections;

public class EmployeeCompare implements Comparable<EmployeeCompare>{
	int eid;
	String ename;
	
	EmployeeCompare(int eid,String name){
		this.eid=eid;
		this.ename=ename;
	}
	public String toString() {
        return "ID: " + eid + " | Name: " + ename;

	}
	
	// override the compareTo
	@Override
	public int compareTo(EmployeeCompare e) {
		return this.cid-c.cid;             //ascending Order
//		return c.cid-this.cid;            // descending order
	}
}
