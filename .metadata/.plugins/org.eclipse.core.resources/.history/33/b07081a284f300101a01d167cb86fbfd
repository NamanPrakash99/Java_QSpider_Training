package Collections;

import java.util.LinkedList;

public class llExample4 {
	public static void main(String[] args) {
		LinkedList<String> list = new LinkedList<>();
		LinkedList<String> list1 = new LinkedList<>();

		
		list.add("Naman");
		list.add("Ravi");
		list.add("Divyansh");
		System.out.println(list);
		
		list1.add("ayush");
		list1.add("xyz");
		list1.add("abc");
		System.out.println(list1);

		// adding the elements in linked list
		list.add("utkarsh");
        list.addFirst("shivansh");
        list.addLast("Dingii");
        
        list.addAll(list1);
        System.out.println(list);
        
        // removing the elements in a linked list
        
        

	}
}
