package AnonymousClass;

public class CalciMain {
	public static void main(String[] args) {
		Calci c=new Calci() {
			@Override
			public void add() {
				int x=10;
				int y=20;
				System.out.println("Adiition: "x+y);
			}
			@Override
			public void printEven(int a,int b) {
				for (int i = a; i <= b; i++) {
                    if (i % 2 == 0) {
                        System.out.print(i + " ");
                    }
			}
		}
	}
}
